[codespell]
# Skip build directories, third-party code, and binary files.
skip = .git,build,.build,*.o,*.a,*.so,*.pyc,blas/f2c,lapack/f2c,CHANGELOG.md
# Ignore words that are false positives in this codebase.
# Variable names: matA, matC, MatA, MatC, ComplexT, ans, padd, padds, ges,
#   workd, whch, bReal, kInf, Indx, pres, dum, ot, Ot, bu, Bu, fo, Fo, ue, Ue,
#   ALS, FOM, NIN, lowd, copie, somme, anc, scond
# Eigen packet names: preverse (packet reverse), padd (packet add)
# Other technical terms: halfs, compiletime, frequence, raisons
ignore-words-list =
    nd,te,ba,ser,
    matA,matC,MatA,MatC,matc,
    ComplexT,bReal,kInf,Indx,
    ans,padd,padds,ges,workd,whch,pres,dum,
    ot,Ot,bu,Bu,fo,Fo,ue,Ue,
    ALS,FOM,NIN,
    lowd,copie,somme,anc,scond,
    preverse,halfs,compiletime,frequence,raisons,
    ded,numer,inpt,overfl,lastr
